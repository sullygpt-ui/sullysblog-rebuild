{
  "rewrites": [
    {
      "source": "/category/domians/feed",
      "destination": "/api/feed?cat=domains"
    },
    {
      "source": "/category/domians/feed/",
      "destination": "/api/feed?cat=domains"
    },
    {
      "source": "/category/domians/feed/atom",
      "destination": "/api/feed?cat=domains"
    },
    {
      "source": "/category/domians/feed/atom/",
      "destination": "/api/feed?cat=domains"
    },
    {
      "source": "/category/:slug/feed",
      "destination": "/api/feed?cat=:slug"
    },
    {
      "source": "/category/:slug/feed/",
      "destination": "/api/feed?cat=:slug"
    },
    {
      "source": "/category/:slug/feed/atom",
      "destination": "/api/feed?cat=:slug"
    },
    {
      "source": "/category/:slug/feed/atom/",
      "destination": "/api/feed?cat=:slug"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/check-expirations",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/publish-scheduled",
      "schedule": "0 * * * *"
    }
  ]
}
